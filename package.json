{
    "name": "daly_bms_application",
    "description": "Application for configuring and monitoringDaly BMS",
    "version": "0.0.0",
    "private": true,
    "author": "ARNAUDIN RÃ©my",
    "proxy": "http://localhost:4000",
    "homepage": ".",
    "build": {
        "asar": true,
        "directories": {
            "output": "out",
            "buildResources": "assets"
        },
        "extraResources": [
            {
                "from": "assets",
                "to": "./"
            }
        ],
        "extraMetadata": {
            "main": "main.js"
        },
        "win": {
            "target": "nsis",
            "icon": "public/favicon.ico",
            "publish": [
                {
                    "provider": "github",
                    "repo": "Daly_Bms_Application",
                    "owner": "remyar",
                    "releaseType": "release"
                }
            ],
            "artifactName": "Daly_Bms_Application-Setup-${version}.${ext}",
            "files": [
                "!out/*",
                "!webpack*",
                "!src/*",
                "!*.log",
                "!node_modules/*",
                "!dev-app-update.yml",
                "!.vscode/*",
                "!.github/*",
                "!*.md",
                "!public/*"
            ]
        }
    },
    "dependencies": {
        "@craco/craco": "^7.1.0",
